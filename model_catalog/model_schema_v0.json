{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Model Schema",
  "type": "object",
  "properties": {
    "provider": { "type": ["string", "null"] },
    "model_name": { "type": ["string", "null"] },

    "modalities": {
      "type": "object",
      "properties": {
        "input": { "type": "array", "items": { "type": "string" } },
        "output": { "type": "array", "items": { "type": "string" } }
      }
    },

    "capabilities": { "type": "array", "items": { "type": "string" } },
    "endpoints": { "type": "array", "items": { "type": "string" } },
    "tools": { "type": "array", "items": { "type": "string" } },
    "snapshots": { "type": "array", "items": { "type": "string" } },

    "training_data_cutoff": { "type": ["string", "null"], "format": "date" },
    "context_window": { "type": ["integer", "null"], "minimum": 0 },
    "max_output_tokens": { "type": ["integer", "null"], "minimum": 0 },
    "reasoning": { "type": ["boolean", "null"] },

    "rate_limits": {
      "type": "object",
      "properties": {
        "tiers": {
          "type": "object",
          "properties": {
            "free": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            },
            "tier_1": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            },
            "tier_2": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            },
            "tier_3": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            },
            "tier_4": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            },
            "tier_5": {
              "type": "object",
              "properties": {
                "rpm": { "type": ["integer", "null"], "minimum": 0 },
                "tpm": { "type": ["integer", "null"], "minimum": 0 },
                "batch_queue_limit": { "type": ["integer", "null"], "minimum": 0 }
              }
            }
          }
        },
        "generic_limits": {
          "type": "object",
          "properties": {
            "rpm": { "type": ["integer", "null"], "minimum": 0 },
            "tpm": { "type": ["integer", "null"], "minimum": 0 }
          }
        }
      }
    },

    "pricing": {
      "type": "object",
      "properties": {
        "text_per_MTok": {
          "type": "object",
          "properties": {
            "batch": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "flex": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "standard": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "priority": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "base": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cache_writes_5m": { "type": ["number", "null"], "minimum": 0 },
                "cache_writes_1h": { "type": ["number", "null"], "minimum": 0 },
                "cache_hits_refreshes": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            }
          }
        },

        "image_per_MTok": {
          "type": "object",
          "properties": {
            "input": { "type": ["number", "null"], "minimum": 0 },
            "cached_input": { "type": ["number", "null"], "minimum": 0 },
            "output": { "type": ["number", "null"], "minimum": 0 }
          }
        },

        "audio_per_MTok": {
          "type": "object",
          "properties": {
            "input": { "type": ["number", "null"], "minimum": 0 },
            "cached_input": { "type": ["number", "null"], "minimum": 0 },
            "output": { "type": ["number", "null"], "minimum": 0 }
          }
        },

        "fine_tuning_per_MTok": {
          "type": "object",
          "properties": {
            "batch": {
              "type": "object",
              "properties": {
                "training": { "type": ["number", "null"], "minimum": 0 },
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "standard": {
              "type": "object",
              "properties": {
                "training": { "type": ["number", "null"], "minimum": 0 },
                "input": { "type": ["number", "null"], "minimum": 0 },
                "cached_input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            }
          }
        },

        "embeddings_per_MTok": {
          "type": "object",
          "properties": {
            "cost": { "type": ["number", "null"], "minimum": 0 },
            "batch_cost": { "type": ["number", "null"], "minimum": 0 }
          }
        },

        "legacy_models_per_MTok": {
          "type": "object",
          "properties": {
            "batch": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "standard": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            }
          }
        },

        "per_image": { "type": ["number", "null"], "minimum": 0 },

        "feature_specific": {
          "type": "object",
          "properties": {
            "batch_processing": {
              "type": "object",
              "properties": {
                "input": { "type": ["number", "null"], "minimum": 0 },
                "output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "long_context": {
              "type": "object",
              "properties": {
                "<=200k_input": { "type": ["number", "null"], "minimum": 0 },
                "<=200k_output": { "type": ["number", "null"], "minimum": 0 },
                ">200k_input": { "type": ["number", "null"], "minimum": 0 },
                ">200k_output": { "type": ["number", "null"], "minimum": 0 }
              }
            },
            "tool_use": {
              "type": "object",
              "properties": {
                "auto_none": { "type": ["number", "null"], "minimum": 0 },
                "any_tool": { "type": ["number", "null"], "minimum": 0 }
              }
            }
          }
        },

        "tools": {
          "type": "object",
          "properties": {
            "code_interpreter": { "type": ["number", "null"], "minimum": 0 },
            "file_search_storage": { "type": ["number", "null"], "minimum": 0 },
            "file_search_call": { "type": ["number", "null"], "minimum": 0 },
            "web_search": { "type": ["number", "null"], "minimum": 0 },
            "web_search_preview": { "type": ["number", "null"], "minimum": 0 },
            "mcp_connector": { "type": ["number", "null"], "minimum": 0 },
            "bash": { "type": ["number", "null"], "minimum": 0 },
            "code_execution": { "type": ["number", "null"], "minimum": 0 },
            "computer_use": { "type": ["number", "null"], "minimum": 0 },
            "text_editor": { "type": ["number", "null"], "minimum": 0 },
            "web_fetch": { "type": ["number", "null"], "minimum": 0 },
            "livesearch": { "type": ["number", "null"], "minimum": 0 }
          }
        }
      }
    },

    "last_updated": { "type": ["string", "null"], "format": "date-time" }
  }
}
