{
  "name": "ekai-gateway",
  "version": "0.1.0-beta.1",
  "description": "Ekai Gateway - Multi-provider AI request routing with dashboard",
  "private": true,
  "workspaces": ["gateway", "ui/dashboard", "memory"],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=gateway\" \"npm run dev --workspace=ui/dashboard\"",
    "dev:core": "concurrently \"npm run dev --workspace=gateway\" \"npm run dev --workspace=ui/dashboard\"",
    "dev:all": "concurrently \"npm run dev --workspace=gateway\" \"npm run dev --workspace=ui/dashboard\" \"npm run start --workspace=memory\"",
    "build": "npm install --workspaces && npm run build --workspaces --if-present",
    "start": "concurrently \"npm run start:gateway\" \"npm run start:ui\" \"npm run start:memory\"",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "test": "npm run test --workspace=gateway",
    "dev:gateway": "npm run dev --workspace=gateway",
    "dev:ui": "npm run dev --workspace=ui/dashboard",
    "start:gateway": "npm run start --workspace=gateway",
    "start:ui": "npx --workspace=ui/dashboard next start -p 3000 -H 0.0.0.0",
    "start:memory": "npm run start --workspace=memory"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "ai",
    "proxy",
    "gateway",
    "dashboard",
    "openai",
    "anthropic",
    "openrouter"
  ],
  "author": "Ekai Labs"
}
